<table style="border: 2px solid black;">
  <tr style="border: 2px solid black;">
      <td>ID</td>
      <td>Title</td>
      <td>DueDate</td>
      <td>Completed</td>
  </tr>
  <% for( var n = 0; n < allTodos.length; n++) {%>
      <tr style="border: 2px solid black;">
          <td><%= allTodos[i].id %></td>
          <td><%= allTodos[i].title %></td>
          <td><%= allTodos[i].duedate %></td>
          <td><%= allTodos[i].completed %></td>
      </tr>
  <% } %>
</table>
